<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>


    <style name="feedback_NoTitle" parent="android:Theme.Light">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="feedback_ListViewStyle">
        <item name="android:scrollbarStyle">outsideOverlay</item>
    </style>
    
   
    
  
	
	<!-- DTS2014123001309 zhouxin/wx230501 20150105 begin -->
    <style name="feedback_compatibleView">
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>       
    </style>
    <!-- DTS2014123001309 zhouxin/wx230501 20150105 end -->
    
    <style name="feedback_section_divide_style">
        <item name="android:textColor">@color/feedback_text_color_75</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">HwChinese-medium</item>
    </style>
   
</resources>