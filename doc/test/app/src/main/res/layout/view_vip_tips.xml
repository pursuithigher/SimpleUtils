<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_bottom_tips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/akpay_warm_prompt_only_simplified_chinese"
        android:textColor="@color/color_100_000000"
        android:textSize="@dimen/hw_text_15" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/hw_vip_tips_1"
        android:textColor="@color/color_50_000000"
        android:textSize="@dimen/hw_text_13" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/hw_vip_tips_2"
        android:textColor="@color/color_50_000000"
        android:textSize="@dimen/hw_text_13" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:lineSpacingMultiplier="1.3"
        android:text="@string/hw_vip_tips_3"
        android:textColor="@color/color_50_000000"
        android:textSize="@dimen/hw_text_13" />

    <TextView
        android:id="@+id/tv_userId_msg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:lineSpacingMultiplier="1.3"
        android:text="@string/hw_vip_tips_4"
        android:textColor="@color/color_50_000000"
        android:textSize="@dimen/hw_text_13" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:lineSpacingMultiplier="1.3"
        android:text="@string/hw_vip_tips_5"
        android:textColor="@color/color_50_000000"
        android:textSize="@dimen/hw_text_13" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:lineSpacingMultiplier="1.3"
        android:text="@string/hw_vip_tips_6"
        android:textColor="@color/color_50_000000"
        android:textSize="@dimen/hw_text_13" />

</merge>